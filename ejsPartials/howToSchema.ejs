<!-- HowTo Schema for Transfer Guides -->
<script type="application/ld+json">
{
  "@context": "https://schema.org",
  "@type": "HowTo",
  "name": "<%= howToTitle %>",
  "description": "<%= howToDescription %>",
  "image": "https://yourdomain.com/<%= howToImage || 'og-image.jpg' %>",
  "totalTime": "<%= howToDuration || 'PT10M' %>",
  "estimatedCost": {
    "@type": "MonetaryAmount",
    "currency": "USD",
    "value": "0"
  },
  "supply": <%= JSON.stringify(howToSupplies || []) %>,
  "tool": <%= JSON.stringify(howToTools || []) %>,
  "step": <%= JSON.stringify(howToSteps) %>
}
</script>